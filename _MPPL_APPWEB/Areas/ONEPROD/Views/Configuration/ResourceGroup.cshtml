@using MDL_ONEPROD.Model.Scheduling
@using GridMvc.Html

@model _MPPL_WEB_START.Areas.ONEPROD.Base.ViewModels.ResourceGroupViewModel
@{
    ViewBag.Title = "CONFIG: Obszary";
}

<div id="configMenu">
    @{ Html.RenderAction("Index"); }
</div>

<div class="row">
    <div class="col-lg-12" style="width: 800px;">
        <div id="areasGrid"></div>
    </div>
</div>


<script type="text/javascript">

    var resourceGroup = new ResourceGroup("#areasGrid");
    $(document).ready(function () {
        resourceGroup.InitGrid();
        resourceGroup.RefreshGrid();
    });
    
</script>


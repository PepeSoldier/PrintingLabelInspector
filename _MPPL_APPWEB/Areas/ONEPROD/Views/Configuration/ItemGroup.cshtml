@using MDL_ONEPROD.Model.Scheduling
@using GridMvc.Html

@model _MPPL_WEB_START.Areas.ONEPROD.Base.ViewModels.ItemGroupViewModel

@{
    ViewBag.Title = "CONFIG: Grupy artyk.";
}

<div id="configMenu">
    @{ Html.RenderAction("Index"); }
</div>


<div class="row">
    <div class="col-lg-12" style="width: 1200px;">
        <div id="ItemGroupGrid"></div>
    </div>
</div>

@section Scripts{
    <script src="~/_ClientAppJS/_APPWEB/Common/MyColorPicker.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            var ItemGroups = new ItemGroup("#ItemGroupGrid");
            ItemGroups.InitGrid();
            ItemGroups.RefreshGrid();
        });

    </script>
}
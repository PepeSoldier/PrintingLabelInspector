
<div class="ganttchartItem" orderno="{{Number}}"
     style="width: 100%;"
     data-toggle="popover" title="{{Number}}"
     data-trigger="hover"
     data-content="Code${{ItemCode}}%Order${{Number}}%Name${{ItemName}}%Qty${{Qty_Remain}} / {{Qty_Total}}%StartTime${{StartTime|FormatDotNetDate}}%EndTime${{EndTime|FormatDotNetDate}}%ProdTime${{ProcessingTime}} [s]"
     >

    <div style="position: absolute;top: 0;left: 0; z-index: 6;">
        <span class="WorkorderCaption">{{ItemCode}}</span>
        <span class="WorkorderCaption">{{OrderNo}}</span>
    </div>

    {{#isStarted}}
        <div class="orderUsed" style="width: {{width}}%;"></div>
    {{/isStarted}}
    
</div>
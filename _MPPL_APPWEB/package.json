{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "watch": {
    "build": "Areas/iLOGIS/Views/.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "watch": "npm-watch",
    "startwatch": "webpack --config webpack.config.js --watch --progress --profile",
    "build": "webpack --config webpack.config.js --progress --profile",
    "prod": "webpack --env.NODE_ENV=production --config webpack.config.js --progress",
    "publish": "webpack-dev-server --output-public-path=/dev/",
    "build:watch": "webpack -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "angular-in-memory-web-api": "^0.5.4",
    "core-js": "^2.6.11",
    "rxjs": "5.5.6",
    "systemjs": "0.20.19",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jasmine": "2.8.6",
    "@types/node": "^9.6.55",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "canonical-path": "0.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^3.6.1",
    "html-webpack-plugin": "^4.2.0",
    "jasmine-core": "~2.9.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.5.4",
    "lodash": "^4.17.15",
    "npm-watch": "^0.6.0",
    "protractor": "^6.0.0",
    "rimraf": "^2.7.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {}
}
